local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "MRWHOHACKS", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

OrionLib:MakeNotification({
	Name = "SUBSCRIBE TO MRWHOHACKS",
	Content = " Made by MRWHOHACKS??",
	Image = "rbxassetid://7742274679",
	Time = 10
})

local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "auto shove",
	Callback = function()
      		while true do
local args = {
    [1] = {
        ["Action"] = "Shove"
    }
}

game:GetService("ReplicatedStorage").Remotes.LineRemote:FireServer(unpack(args))
wait()
end
  	end    
})

Tab:AddParagraph("gift","Free gift")

